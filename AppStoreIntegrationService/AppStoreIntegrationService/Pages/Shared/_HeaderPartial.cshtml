@using Microsoft.Extensions.Options;
@using AppStoreIntegrationService.Model;
@inject IOptions<SiteSettings> options;
@{
    var siteName = options.Value.Name;
}
@if (string.IsNullOrEmpty(siteName))
{
    <h2 style="color:white">PA Admin</h2>
}
else
{
    <h2 style="color:white">PA Admin - @siteName</h2>
}
